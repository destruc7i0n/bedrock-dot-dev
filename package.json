    "@docsearch/react": "^3.9.0",
    "@heroicons/react": "^2.2.0",
    "@nanostores/react": "^1.0.0",
    "astro": "^5.15.4",
    "astro-nprogress": "^0.0.2",
    "astro-react-i18next": "^0.3.3",
    "classnames": "^2.5.1",
