    "classnames": "^2.5.1",
    "element-in-view": "^0.1.0",
    "flat-cache": "^6.1.18",
    "next": "16.0.0",
    "next-intl": "^4.4.0",
    "next-themes": "^0.4.6",
    "nprogress": "^0.2.0",
    "prismjs": "^1.30.0",
    "react": "19.2.0",
    "react-dom": "19.2.0",
